{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:ws": "node server-ws.js",
    "dev:ws:v2": "tsx server-ws-v2.js",
    "dev:worker": "tsx worker.js --daemon",
    "dev:all": "npm run dev:ws:v2 & npm run dev:worker & npm run dev",
    "dev:redis": "docker-compose up -d redis",
    "stop:redis": "docker-compose down",
    "worker": "tsx worker.js",
    "worker:daemon": "tsx worker.js --daemon",
    "build": "npm run build:next && npm run build:worker",
    "build:next": "next build",
    "build:worker": "tsc --project tsconfig.worker.json",
    "start": "next start",
    "start:ws": "NODE_ENV=production node server-ws-v2.js",
    "start:worker": "NODE_ENV=production node worker.js --daemon",
    "lint": "eslint"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.28.0",
    "@drift-labs/sdk": "^2.143.0-beta.4",
    "@prisma/client": "^6.17.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-switch": "^1.2.6",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.546.0",
    "next": "15.5.6",
    "next-themes": "^0.4.6",
    "postprocessing": "^6.37.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "recharts": "^3.3.0",
    "redis": "^5.9.0",
    "tailwind-merge": "^3.3.1",
    "three": "^0.180.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/bn.js": "^5.2.0",
    "@types/node": "^20.19.22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.5.13",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "prisma": "^6.17.1",
    "tailwindcss": "^4",
    "tsx": "^4.19.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
